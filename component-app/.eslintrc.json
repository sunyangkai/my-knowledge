{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
    },
    "globals": {
        "__dirname": true,
        "process": true,
        "require": true,
        "module": true,
        "exports": true,
        "console": true,
        "window": true,
        "document": true,
        "navigator": true,
        "fetch": true,
        "setTimeout": true,
        "setInterval": true,
        "clearTimeout": true,
        "clearInterval": true,
        "Promise": true,
        "FormData": true,
        "XMLHttpRequest": true,
        "MY_GLOBAL": true
      }
}
